@model OnlineExam.Models.Student

@{
    ViewData["Title"] = "Profile";
}

<h1>Profile</h1>

<div class="row">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label asp-for="FullName" class="control-label"></label>
                <input asp-for="FullName" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="EnrollmentDate" class="control-label"></label>
                <input asp-for="EnrollmentDate" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="Course" class="control-label"></label>
                <input asp-for="Course" class="form-control" readonly />
            </div>
            <br />
            <div class="form-group">
                <a asp-action="ChangePassword" >Change Password</a>
            </div>
        </form>
    </div>
</div>
<br />
<div>
    <a asp-action="Index">Back to List</a>
</div>
